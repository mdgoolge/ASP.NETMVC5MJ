@model IEnumerable<Mvc5Examples.Areas.Chapter08.Models.MyDb2Model.MyTable1>

<h2>课程编码管理</h2>
<table class="table table-bordered">
    <tr>
        <th>@Html.DisplayNameFor(model => model.KeChengID)</th>
        <th>@Html.DisplayNameFor(model => model.KeChengName)</th>
        <th>操作</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.KeChengID)</td>
            <td>@Html.DisplayFor(modelItem => item.KeChengName)</td>
            <td>
                @Html.ActionLink("编辑", "Edit", new { id = item.KeChengID }) |
                @Html.ActionLink("删除", "Delete", new { id = item.KeChengID })
            </td>
        </tr>
    }
    <tr>
        <td colspan="4" class="text-center">
            @Html.ActionLink("添加新行", "Create", null, new {@class="btn btn-success" })
        </td>
    </tr>
</table>
