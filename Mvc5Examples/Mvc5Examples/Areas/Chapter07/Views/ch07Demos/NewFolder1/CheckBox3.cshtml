
<h2>jQueryUI复选按钮基本用法（btnCheckBox3）</h2>
<input type="checkbox" id="check"><label id="label1" for="check">选中/不选中切换</label>
<hr />
<div id="format">
    <input type="checkbox" id="check1"><label for="check1">选项1</label>
    <input type="checkbox" id="check2"><label for="check2">选项2</label>
    <input type="checkbox" id="check3"><label for="check3">选项3</label>
</div>
<script>
    $.ui.buttonset(null, "#check");
    $.ui.buttonset(null, "#format");
    $("#check").change(function () {
        if ($("#check").prop("checked") == true) {
            $("#label1").text("选中状态");
        }
        else {
            $("#label1").text("未选中状态");
        }
    });
